<% provide(:title, 'Sign up') %>
<h1>会議に参加する</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for @users, :url => {:action => 'create'} do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %>
      <%= hidden_field_tag :discuss_id, @users.discuss_id %>
      <%= f.submit "参加する", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
